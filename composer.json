{
    "name": "flashytime/curl",
    "description": "A lightweight CURL wrapper for php",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "flashytime",
            "email": "myflashytime@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6",
        "ext-curl": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^5"
    },
    "autoload": {
        "psr-4": {
            "Flashytime\\Curl\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Flashytime\\Curl\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
